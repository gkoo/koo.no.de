#content.show
  #blogSection
    h2.sectionTitle it's a blog
    .blogContent
    ul.posts
      != partial('blogpost', posts)
    - if (typeof prevLink !== 'undefined' && prevLink !== '')
      a.prev.blog-nav(href=prevLink) prev
    - if (typeof nextLink !== 'undefined' && nextLink !== '')
      a.next.blog-nav(href=nextLink) next

  a#homeBtn.show(href="/")
    img(src="/img/drawericons/home_icon.png")
